#table
  %table
    -@mixtape.songs.each do |song|
      %tr
        %td= link_to(song.name, song_path(song))
        %td= audio_tag(song.filename, :controls => true)
        %td= link_to('Refund', song, :method => :delete, :confirm => 'Are you sure you want to sell back this song at 30% less the purchase price?', :class => 'button radius tiny')
        %p
